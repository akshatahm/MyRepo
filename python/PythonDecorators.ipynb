   "id": "77f1bcf7-20a7-4169-9b10-f6d3029f92c7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def log(fun):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        print(f' Logging function: {fun.__name}, args:{args}, kwargs:{kwargs}')\n",
    "\n",
    "    return wrapper\n",
    "\n",
    "\n",
    "@log\n",
    "def add(a,b):\n",
    "    return a+b\n",
    "\n",
    "add(5,2)"
   ]
  }
